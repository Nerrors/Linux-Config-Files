	#Custom Shortcuts

#Rofi (Alt + Space) (Default Alt + Space Shortcut Activate window menu (Category: Windows))
rofi -combi-modi drun,window,ssh -theme "~/.config/rofi/rofi-themes/Official Themes/purple" -font "hack 11" -show combi -show-icons -lines 14 -columns 2 -width 1300 -location 0
rofi -combi-modi drun,window,ssh -font "hack 11" -show combi -show-icons -lines 12 -columns 1 -width 1250 -location 0

#Conky (Super + C)
conky

#Kill Conky (Alt + C)
killall conky

#Dialect (Super + T)
flatpak run com.github.gi_lom.dialect
/usr/bin/flatpak run --branch=stable --arch=x86_64 --command=dialect com.github.gi_lom.dialect

#Terminal Emulator (Ctrl + Alt + T), (Super + Return(Enter))
alacritty

#Screenshot (Super + Print)
gnome-screenshot --interactive

#System Monitor (Ctrl + Shift + Escape)
gnome-system-monitor

#System Settings (Super + I)
cinnamon-settings

#Shutdown (Ctrl + Alt + End)
shutdown -h now

#Spotify (Super + S)
flatpak run com.spotify.Client
/usr/bin/flatpak run --branch=stable --arch=x86_64 --command=spotify --file-forwarding com.spotify.Client @@u %U @@
